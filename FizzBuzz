print("Dobro došli u FizzBuzz igru.")

game = input("Upišite jedan broj između 1 i 100.")
game = int(game)

for broj in range(1, game+1):
    if broj %3 == 0 and broj %5 == 0:
        print("fizzbuzz")
    elif broj %3 == 0:
        print("fizz")
    elif broj %5 == 0:
        print("buzz")
    else:
        print(broj)
